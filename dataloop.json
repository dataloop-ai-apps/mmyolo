{
  "name": "mmyolo",
  "displayName": "MMYolo Model",
  "version": "v0.0.0",
  "scope": "public",
  "codebase": {
    "type": "git",
    "gitUrl": "https://github.com/dataloop-ai-apps/mmyolo.git",
    "gitTag": "v0.0.0"
  },
  "description": "MMYolo Model Application by OpenMMLab",
  "attributes": {
    "Provider": "OpenMMLab",
    "Category": "Model",
    "Computer Vision": "Object Detection",
    "License": "Apache 2.0",
    "Media Type": "Image"
  },

  "components": {
    "computeConfigs": [
      {
        "name": "mmyolo-deploy",
        "runtime": {
          "podType": "regular-xs",
          "concurrency": 1,
          "runnerImage": "shadimahameeddl/mm-dl:latest",
          "autoscaler": {
            "type": "rabbitmq",
            "minReplicas": 0,
            "maxReplicas": 2
          }
        }
      }
    ],
    "modules": [
      {
        "name": "mmyolo-module",
        "entryPoint": "model_adapter.py",
        "className": "MMYolo",
        "computeConfig": "mmyolo-deploy",
        "description": "MMYolo Module",
        "initInputs": [
          {
            "type": "Model",
            "name": "model_entity"
          }
        ],
        "functions": [
          {
            "name": "predict_items",
            "input": [
              {
                "type": "Item[]",
                "name": "items",
                "description": "List of items to run inference on"
              }
            ],
            "output": [
              {
                "type": "Annotations[]",
                "name": "annotations",
                "description": "List of predictions"
              }
            ],
            "displayName": "Predict Items",
            "displayIcon": "",
            "description": "MMYolo predict function"
          }
        ]
      }
    ],
    "models": [
      {
        "name": "mmlab-mmyolo",
        "moduleName": "mmyolo-module",
        "scope": "project",
        "status": "trained",
        "configuration": {
          "model_name": "yolox_s_fast_8xb8-300e_coco",
          "config_file": "yolox_s_fast_8xb8-300e_coco.py",
          "checkpoint_file": "yolox_s_fast_8xb8-300e_coco_20230213_142600-2b224d8b.pth",
          "device": "cpu",
          "confidence_thr": 0.4
        },
        "supportedMethods": {
          "load": true,
          "predict": true,
          "train": false,
          "evaluate": true
        },
        "description": "MMYolo is an open source object detection toolbox based on PyTorch. It is a part of the OpenMMLab project.",
